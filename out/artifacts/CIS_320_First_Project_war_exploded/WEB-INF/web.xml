<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>NameListGet</servlet-name>
        <servlet-class>edu.simpson.computerscience.webdevelopment.NameListGet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NameListGet</servlet-name>
        <url-pattern>/api/get_name_list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NameListEdit</servlet-name>
        <servlet-class>edu.simpson.computerscience.webdevelopment.NameListEdit</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NameListEdit</servlet-name>
        <url-pattern>/api/name_list_edit</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>FormTestServlet</servlet-name>
        <servlet-class>edu.simpson.computerscience.webdevelopment.FormTestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FormTestServlet</servlet-name>
        <url-pattern>/api/form_test_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FormTestJSONServlet</servlet-name>
        <servlet-class>edu.simpson.computerscience.webdevelopment.FormTestJSONServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FormTestJSONServlet</servlet-name>
        <url-pattern>/api/form_test_json_servlet</url-pattern>
    </servlet-mapping>
</web-app>